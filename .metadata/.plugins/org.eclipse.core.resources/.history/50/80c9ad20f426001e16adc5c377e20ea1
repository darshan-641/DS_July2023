
public class AccountTest {
	public static void main(String[] args) {
		
		int bookNumber1=123;
		String bookName1 = "Java Programming Language";
		String author1 ="James Gosling";
		float price1=890.0f;
		int numberOfPages1=1200;
		int edition1=1;
		
		int bookNumber2=123;
		String bookName2 = "Unix";
		String author2 ="Richard Stallman";
		float price2=1290.0f;
		int numberOfPages2=800;
		int edition2=1;
		
		
		System.out.println("Number  : "+bookNumber1);
		System.out.println("Name    : "+bookName1);
		System.out.println("Author  : "+author1);
		System.out.println("Price   : "+price1);
		System.out.println("NOP     : "+numberOfPages1);
		System.out.println("Edition : "+edition1+"\n");
		
		System.out.println("Number  : "+bookNumber2);
		System.out.println("Name    : "+bookName2);
		System.out.println("Author  : "+author2);
		System.out.println("Price   : "+price2);
		System.out.println("NOP     : "+numberOfPages2);
		System.out.println("Edition : "+edition2+"\n");
		
	}
}
