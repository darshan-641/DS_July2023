
public class TigerTest {
	public static void main(String[] args) {
		
		Tiger tiger = new Tiger();
		tiger.jump();
		tiger.jump(10);
	}
}

class Tiger
{
	void jump() {
		System.out.println("Tiger is jumping...");
	}
	void jump(int length) {
		System.out.println("Tiger is jumping..."+length+" feet long");
	}
}
