
public class TigerTest {
	public static void main(String[] args) {
		
		Tiger tiger = new Tiger();
		tiger.jump();
		tiger.jump(10);
		tiger.jump(12, 6);
		tiger.jump(9.5f);
	}
}

class Tiger
{
	void jump() {
		System.out.println("Tiger is jumping...");
	}
	void jump(int length) {
		System.out.println("Tiger is jumping..."+length+" feet length");
	}
	void jump(int length,int height) {
		System.out.println("Tiger is jumping..."+length+" feet length and  "+height+" feet long height");
	}
	void jump(float length) {
		System.out.println("Tiger is jumping..."+length+" feet length");
	}
}
