
public class AccountTest {
	public static void main(String[] args) {
		
		int bookNumber=123;
		String bookName = "Java Programming Language";
		String author ="James Gosling";
		float price=890.0f;
		int numberOfPages=1200;
		int edition=1;
		
		System.out.println("Book Number  : "+bookNumber);
		System.out.println("Book Name    : "+bookName);
		System.out.println("Book Author  : "+author);
		System.out.println("Book Price   : "+price);
		System.out.println("Book NOP     : "+numberOfPages);
		System.out.println("Book Edition : "+edition);
		
	}
}
